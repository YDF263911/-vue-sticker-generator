{
  "version": 2,
  "builds": [
    {
      "src": "vue-sticker-generator/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "vue-sticker-generator/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vue-sticker-generator/dist/index.html"
    }
  ]
}